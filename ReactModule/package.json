{
  "name": "reactmodule",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/react-router-dom": "^5.1.8",
    "@ui5/webcomponents": "^1.0.0-rc.14",
    "@ui5/webcomponents-fiori": "1.0.0-rc.14",
    "@ui5/webcomponents-icons": "1.0.0-rc.14",
    "@ui5/webcomponents-react": "^0.16.6",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.0",
    "styled-normalize": "^8.0.7"
  },
  "scripts": {
    "build": "yarn run clean && yarn run build:react && yarn run zip",
    "clean": "npx rimraf ReactModule-content.zip build",
    "zip": "npx bestzip ReactModule-content.zip build/*",
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "test:react": "react-scripts test --env jest-environment-jsdom-sixteen",
    "eject:react": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(@ui5|lit-html)).*\\.js$"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.1",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.11",
    "typescript": "^4.3.5"
  }
}
